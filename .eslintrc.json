{
  "extends": ["@rocketseat/eslint-config/react"],
  "rules": {
    "no-undef": "off",
    "camelcase": "off",
    "no-useless-constructor": "off",
    "prettier/prettier": [
      0,
      {
        "printWidth": 85,
        "tabWidth": 2,
        "singleQuote": true,
        "trailingComma": "all",
        "arrowParens": "always",
        "semi": false
      }
    ]
  }
}
